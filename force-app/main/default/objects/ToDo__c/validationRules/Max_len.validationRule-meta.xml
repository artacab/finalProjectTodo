<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Max_len</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(Name) &gt; 20</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>The length is too long</errorMessage>
</ValidationRule>
